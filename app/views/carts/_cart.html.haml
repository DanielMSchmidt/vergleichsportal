%li.active{:id => "cart-#{cart.id}"}
  .title
    %h5
      = cart.id
  .content
    .row
      .six.columns
        %ul
          -cart.articles.each do |article|
            %li
              =article.name
      .four.columns
        %a.button.expand{:href => "#", :style => "margin-left: 50px; margin-bottom: 15px;"} Auswählen
        %a.button.expand{:href => "#", :style => "margin-left: 50px;"} Löschen
      .two.colums